{
  "name": "jojo1981/jms-serializer-handlers",
  "description": "Handlers for the JMS Serializer",
  "keywords": [
    "jms",
    "serializer",
    "handlers",
    "doctrine",
    "collection",
    "typed",
    "typed-collection"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4|^8.0",
    "jms/serializer": "^3.0",
    "jojo1981/typed-collection": "^2.0",
    "jojo1981/typed-set": "^1.6"
  },
  "require-dev": {
    "doctrine/cache": "^1.8",
    "php-coveralls/php-coveralls": "^2.4",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\JmsSerializerHandlers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\Jojo1981\\JmsSerializerHandlers\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "scripts": {
    "clear": [
      "rm -fR ./var/cache"
    ],
    "test": [
      "@clear",
      "./bin/phpunit"
    ],
    "post-install-cmd": [
      "@clear"
    ],
    "post-update-cmd": [
      "@clear"
    ]
  }
}
