{
  "name": "jojo1981/jms-serializer-handlers",
  "description": "Handlers for the JMS Serializer",
  "keywords": [
    "jms",
    "serializer",
    "handlers",
    "doctrine",
    "collection",
    "typed",
    "typed-collection"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "jms/serializer": "^1.14",
    "jojo1981/typed-collection": "^1.0"
  },
  "require-dev": {
    "doctrine/cache": "^1.8",
    "php-coveralls/php-coveralls": "2.1.x-dev",
    "phpunit/phpunit": "7.5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\JmsSerializerHandlers\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "./bin/phpunit"
    ]
  }
}
